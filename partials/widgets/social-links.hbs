<div class="col-two tab-1-3 mob-1-2 social-links">
    <h4>{{t "Social"}}</h4>
    <ul>
        {{!-- Facebook and twitter links will come from dashboard --}}
        {{#if @site.facebook}}
            <li><a href="{{ facebook_url }}">{{t "Facebook"}}</a></li>
        {{/if}}
        {{#if @site.twitter}}
            <li><a href="{{ twitter_url }}">{{t "Twitter"}}</a></li>
        {{/if}}
        <li>
            <a href="#"></a>
            <a href="https://www.linkedin.com/company/electronthemes">{{t "Instagram"}}</a>
        </li>
        <li>
            <a href="https://www.pinterest.com/electronthemes/">{{t "pinterest-p"}}</a>
        </li>
    </ul>
</div> <!-- end social links -->